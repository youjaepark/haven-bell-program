from pygame import mixer
import schedule
import time
import datetime

mixer.init()

def p1():
    mixer.music.load("bell1.mp3") #음악 1 재생
    mixer.music.play()
    time.sleep(9)
    mixer.music.stop()
    
schedule.every().thursday.at("13:59").do(p1)
schedule.every().thursday.at("14:00").do(p1)
        
while True:
    schedule.run_pending()